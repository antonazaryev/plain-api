{
  "name": "api-call",
  "version": "1.0.0",
  "description": "Thin wrapper around your favorite HTTP client to simplify your api calls ",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm test -- -w",
    "init": "mkdir dist",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && npm run init",
    "build": "./node_modules/.bin/babel ./src -d ./dist --ignore test.js",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naorye/api-call.git"
  },
  "keywords": [
    "api",
    "request",
    "axios",
    "superagent",
    "fetch",
    "resource"
  ],
  "author": "NaorYe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/naorye/api-call/issues"
  },
  "homepage": "https://github.com/naorye/api-call#readme",
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^23.1.0"
  },
  "files": [
    "dist"
  ]
}
